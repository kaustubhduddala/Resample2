{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "resample",
  "version": "0.0.1-alpha.1",
  "identifier": "com.resample.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Resample",
        "width": 1150,
        "height": 1005,
        "resizable": false,
        "fullscreen": false,
        "maximizable": false,
        "minimizable": true,
        "decorations": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/icon.png",
      "icons/icon2.png"
    ],
    "resources": {
      "resources/ffmpeg/ffmpeg": "resources/ffmpeg/ffmpeg",
      "resources/ffmpeg/ffprobe": "resources/ffmpeg/ffprobe",
      "resources/ffmpeg/ffmpeg.exe": "resources/ffmpeg/ffmpeg.exe",
      "resources/ffmpeg/ffprobe.exe": "resources/ffmpeg/ffprobe.exe",
      "resources/yt-dlp/dist/yt-dlp": "resources/yt-dlp/dist/yt-dlp",
      "resources/audio-separator/dist/audio-separator": "resources/audio-separator/dist/audio-separator"
    }
  }
} 